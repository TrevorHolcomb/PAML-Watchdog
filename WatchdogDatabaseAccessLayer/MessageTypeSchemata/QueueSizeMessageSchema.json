{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Queue Size Message Schema",
  "description": "schema for validating queue size messages",
  "type": "object",
      "properties": {
        "messageTypeId": {
          "type": "integer",
          "maximum": 0
        },
        "params": {
          "type": "object",
          "queueSize": {
            "type": "integer",
            "minimum": 0
          }
        }
      },
  "required": [ "messageTypeId", "params" ]
}